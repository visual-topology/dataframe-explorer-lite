            "resources": ["nodes/select_rows.js"]
        },
        "join_rows": {
            "metadata": {
                "name": "Join Rows",
                "description": "Join rows from multiple inputs"
            },
            "input_ports": {
                "data_in1": {
                    "allow_multiple_connections": false,
                    "link_type": "dataframe_explorer:dataset"
                },
                "data_in2": {
                    "allow_multiple_connections": false,
