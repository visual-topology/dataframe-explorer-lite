{
    "id": "dataframe_explorer",
    "metadata": {
        "name": "DataFrame Explorer Example Package",
        "version": "0.0.1",
        "description": "A simple example package for building data visualizations",
        "link": "about.html"
    },
    "display": {
        "style": "fill:lightgrey;"
    },
    "executor": "javascript",
    "configuration": {
        "classname": "DataFrameExplorer.Configuration",
        "pages": {
            "default": {
                "button_label": "Configure...",
                "title": "Dataframe Explorer Configuration",
                "url": "configuration.html"
            }
        }
    },
    "l10n": {
        "languages": {
            "en": {
                "name": "English",
                "bundle_url": "l10n/en.json"
            },
            "fr": {
                "name": "French",
                "bundle_url": "l10n/fr.json"
            }
        },
        "default_language": "en"
    },
    "resources": [ "configuration.js", "nodes/chart.js" ],
    "node_types": {
        "csv_import": {
            "metadata": {
                "name": "CSV Reader",
                "description": "Select a CSV file and load it into a dataset"
            },
            "output_ports": {
                "data_out": {
                    "link_type": "dataframe_explorer:dataset"
                }
            },
            "display": {
                "corners": 7,
                "icon": "icons/csv_import.svg"
            },
            "pages": {
                "default": {
                    "menu_label":"Open...",
                    "title": "CSV Import",
                    "url": "nodes/csv_import.html",
                    "window_width": 800,
                    "window_height": 800
                }
            },
            "classname": "DataFrameExplorer.CsvImportNode",
            "resources": ["nodes/csv_import.js"]
        },
        "sample_rows": {
            "metadata": {
                "name": "Sample",
                "description": "Select a sample from the input dataset"
            },
            "input_ports": {
                "data_in": {
                    "allow_multiple_connections": false,
                    "link_type": "dataframe_explorer:dataset"
                }
            },
            "output_ports": {
                "data_out": {
                    "link_type": "dataframe_explorer:dataset"
                }
            },
            "display": {
                "corners": 7,
                "icon": "icons/filter_random.svg"
            },
            "pages": {
                "default": {
                    "menu_label": "Open...",
                    "title": "Sample Rows",
                    "url": "nodes/sample_rows.html",
                    "window_width": 500,
                    "window_height": 300
                }
            },
            "classname": "DataFrameExplorer.SampleRowsNode",
            "resources": ["nodes/sample_rows.js"]
        },
        "select_rows": {
            "metadata": {
                "name": "Select Rows",
                "description": "Select Rows by Category or Numeric Range"
            },
            "input_ports": {
                "data_in": {
